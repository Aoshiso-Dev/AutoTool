<Window x:Class="AutoTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoTool"
        xmlns:panels="clr-namespace:Panels.View;assembly=Panels"
        xmlns:converter="clr-namespace:Panels.View.Converter;assembly=Panels"
        xmlns:vm="MainW"
        mc:Ignorable="d"
        Title="AutoTool" Height="1000" Width="1100"
        ResizeMode="NoResize">

    <Window.DataContext>
        <local:MainWindowViewModel/>
    </Window.DataContext>

    <DockPanel LastChildFill="True">
        <panels:ButtonPanel DataContext="{Binding ButtonPanelViewModel}" DockPanel.Dock="Top"/>
        <panels:LogPanel DataContext="{Binding LogPanelViewModel}" DockPanel.Dock="Bottom" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"/>
        <panels:ListPanel DataContext="{Binding ListPanelViewModel}"/>
    </DockPanel>
</Window>
