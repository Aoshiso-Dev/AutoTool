{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "AutoTool": "Trace",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AutoTool": {
    "Commands": {
      "EnableAttributeBasedRegistration": true,
      "ShowRegistrationDemo": true,
      "VerboseLogging": true
    },
    "Development": {
      "EnableConsoleLogging": true,
      "ShowStartupInfo": true,
      "EnableDebugFeatures": true
    }
  }
}